function permuteString(str, prefix = '', results = []) {
  // Base case: if string is empty, we have a complete permutation
  if (str.length === 0) {
    results.push(prefix);
    return results;
  }
  
  // Recursive case: iterate through each character
  for (let i = 0; i < str.length; i++) {
    // Get current character
    const currentChar = str[i];
    
    // Remove current character from string
    const remainingStr = str.slice(0, i) + str.slice(i + 1);
    
    // Recursively permute the remaining string
    permuteString(remainingStr, prefix + currentChar, results);
  }
  
  // Remove duplicates by converting to Set and back to array
  const uniqueResults = [...new Set(results)];
  
  return uniqueResults;
}
